                "connectionType": {
                    "description": "Relevant for connections only. It describes a type of component itself.\nNote: This does NOT describing the type of a connection referenced in `connection` or `altConnection` property.",
                    "enum": [
                        "basic",
                        "oauth"
                    ],
                    "type": "string"
